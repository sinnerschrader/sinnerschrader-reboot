	<link rel="preconnect" href="https://sinnerschrader.jobs" crossorigin />
	{% include tracking-head %}

	<script async src="bundle.js" type="text/javascript"></script>
	<meta content="width=device-width, initial-scale=1" name="viewport" />
	<meta charset="utf-8" />

	<link rel="mask-icon" href="/assets/meta-assets/favicon.svg" />
	<link rel="alternate icon" type="image/png" href="/assets/meta-assets/favicon.png" />
	<link rel="icon" type="image/svg+xml" href="/assets/meta-assets/favicon.svg" />

	<!-- Primary Meta Tags -->
	<title>SinnerSchrader | Experience Design and Engineering Agency.</title>
	{% if metaTag == 'hiring' %}
	<!-- Primary Meta Tags -->
	<meta name="title" content="SinnerSchrader | We are hiring" />
	<meta
		name="description"
		content="We are looking for Developers in Germany. Join our team of thinkers and doers remotely or support one of our local studios with your Frontend / Fullstack / iOS / Android skills. Say hello"
	/>

	<!-- Open Graph / Facebook -->
	<meta property="og:type" content="website" />
	<meta property="og:url" content="https://sinnerschrader.com/" />
	<meta property="og:title" content="SinnerSchrader | We are hiring" />
	<meta
		property="og:description"
		content="We are looking for Developers in Germany. Join our team of thinkers and doers remotely or support one of our local studios with your Frontend / Fullstack / iOS / Android skills. Say hello"
	/>
	<meta property="og:image" content="https://sinnerschrader.com/assets/meta-assets/recruting_meta.jpg" />
	<meta property="og:image:alt" content="Join. Thinkers that do" />

	<!-- Twitter -->
	<meta property="twitter:card" content="summary_large_image" />
	<meta property="twitter:url" content="https://sinnerschrader.com/" />
	<meta property="twitter:title" content="SinnerSchrader | We are hiring" />
	<meta
		property="twitter:description"
		content="We are looking for Developers in Germany. Join our team of thinkers and doers remotely or support one of our local studios with your Frontend / Fullstack / iOS / Android skills. Say hello"
	/>
	<meta property="twitter:image" content="https://sinnerschrader.com/assets/meta-assets/recruting_meta.jpg" />
	{% else %}
	<meta name="title" content="SinnerSchrader | Experience Design and Engineering Agency." />
	<meta
		name="description"
		content="Born from digital, evolving over 2 decades, SinnerSchrader is a leading design and engineering agency headquartered in Hamburg, Germany."
	/>

	<!-- Open Graph / Facebook -->
	<meta property="og:type" content="website" />
	<meta property="og:url" content="https://sinnerschrader.com/" />
	<meta property="og:title" content="SinnerSchrader | Experience Design and Engineering Agency." />
	<meta
		property="og:description"
		content="Born from digital, evolving over 2 decades, SinnerSchrader is a leading design and engineering agency headquartered in Hamburg, Germany."
	/>
	<meta property="og:image" content="https://sinnerschrader.com/assets/meta-assets/meta_s2_social.jpg" />
	<meta property="og:image:alt" content="SinnerSchrader Part of Accenture Interactive" />

	<!-- Twitter -->
	<meta property="twitter:card" content="summary_large_image" />
	<meta property="twitter:url" content="https://sinnerschrader.com/" />
	<meta property="twitter:title" content="SinnerSchrader | Experience Design and Engineering Agency." />
	<meta
		property="twitter:description"
		content="Born from digital, evolving over 2 decades, SinnerSchrader is a leading design and engineering agency headquartered in Hamburg, Germany."
	/>
	<meta property="twitter:image" content="https://sinnerschrader.com/assets/meta-assets/meta_s2_social.jpg" />
